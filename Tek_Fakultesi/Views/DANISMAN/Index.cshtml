@model List<Tek_Fakultesi.Models.Entity_Fremawork.DANISMAN>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
<div>
    <h2>Personel Bilgi Listesi</h2>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>TC</th>
            <th>Ad</th>
            <th>Soyad</th>
            <th>Unvan</th>
            <th>Birim Adı</th>
            <th>Rol Adı</th>
            <th>Güncelle</th>
            <th>Sil</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var personel in Model)
        {
            <tr>
                
                <td>@personel.KUL_AD</td>
                <td>@personel.AD</td>
                <td>@personel.SOYAD</td>
                <td>@personel.UNVAN</td>
                <td>@personel.BIRIMLER.BIRIMAD</td>
                <td>@personel.ROL.ROLADI</td>
                <td>@Html.ActionLink("Guncelle", "Guncelle", new { id = personel.ID }, new { @class = "btn btn-primary" })</td>
                <td>@Html.ActionLink("Sil","Sil",new { id = personel.ID },new { @class = "btn btn-warning" })</td>

            </tr>
        }


    </tbody>
</table>
@Html.ActionLink("Yeni Personel Ekle", "Yeni", null, new { @class = "btn btn-success"})
    </div>

